<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.david.zhumingwei.chartsdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ColumnChartActivity"></activity>
        <activity android:name=".activity.ComboLineColumnChartActivity"></activity>
        <activity android:name=".activity.LineChartActivity"></activity>
        <activity android:name=".activity.GoodBadChartActivity"></activity>
        <activity android:name=".activity.LineColumnDependencyActivity"></activity>
        <activity android:name=".activity.PieChartActivity"></activity>
        <activity android:name=".activity.PreviewColumnChartActivity"></activity>
        <activity android:name=".activity.PreviewLineChartActivity"></activity>
        <activity android:name=".activity.SpeedChartActivity"></activity>
        <activity android:name=".activity.TempoChartActivity"></activity>
        <activity android:name=".activity.ViewPagerChartsActivity"></activity>
        <activity android:name=".activity.BubbleChartActivity"></activity>

    </application>

</manifest>